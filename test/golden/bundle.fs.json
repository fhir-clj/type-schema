              "type": "code",
              "binding": {
                "strength": "required",
                "valueSet": "http://hl7.org/fhir/ValueSet/search-entry-mode|4.0.1"
              },
              "isSummary": true,
              "index": 12
            },
            "score": {
              "short": "Search ranking (between 0 and 1)",
              "type": "decimal",
              "isSummary": true,
              "index": 13
            }
          }
        },
        "request": {
